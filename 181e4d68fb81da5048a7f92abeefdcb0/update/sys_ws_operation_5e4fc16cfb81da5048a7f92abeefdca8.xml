        <name>Entry Point</name>
        <operation_script><![CDATA[(function process( /*RESTAPIRequest*/ request, /*RESTAPIResponse*/ response) {

    const processor = new SCCInbound(request.body.data).processRequest();

    if (processor.hasError()) {
        response.setError(processor.getServiceError());
        return;
    }

    response.setContentType("application/json");
    response.setStatus(processor.getHTTPCode());
	response.getStreamWriter().writeString(processor.getSuccessfulResponse());

    const transactionMessage = processor.getMessage();
    
    transactionMessage.setValue("http_response_body", processor.getSuccessfulResponse());
    transactionMessage.update();

})(request, response);]]></operation_script>
